<div class="black-header"> <i class="fas fa-bolt"></i><h2>Latest Update</h2></div>
<div class="swiper">
      <div class="latest-movies-section">
       {{#each latestupdatemovies}}
          <a href="/movie/{{this._id}}" class="movies-list-latestupdate"title="{{this.name}} Movie Details">
            <div class="movies-img-latestupdate">
              <img src="/images/movie-posters/{{this._id}}.jpg" alt="{{this.name}} Poster" srcset="">
            </div>
          <div class="movies-body-latestupdate">
            <p>{{this.name}}</p>
            <h6>( {{this.engname}} )</h6>
          </div>  
          </a>
        {{/each}}
      </div>
</div>

<div class="actor-section">
        <div class="movies-english-header"><h1>{{actor.actorname}}</h1></div>
        <div class="actor-two" style="position: relative;">
            <div class="actor-image">
                <img src="/images/actor-posters/{{actor._id}}.jpg" alt="">
            </div>
            <div class="actor-details"  >
                <div class="mv-details">
                    <h4>Name :</h4>  <h5>{{actor.actorname}}</h5>
                </div>
                <div class="mv-details">
                    <h4>Age :</h4> {{#if actor.age}}
        <h5>{{actor.age}}</h5>
        {{else}}
        <h5> Not Show</h5>
        {{/if}} 
                </div>
                <div class="mv-details">
                    <h4>Year Active :</h4>  <h5>{{actor.yearactive}}</h5>
                </div>
                <div class="mv-details">
                    <h4>Language :</h4>  <h5>{{actor.language}}</h5>
                </div>
                <div class="mv-details">
                    <h4>Occupation :</h4>  <h5>{{actor.occupation}}</h5>
                </div>
                <div class="connect-with">
                    <h4>Connect with :</h4> <a href="{{actor.instalink}}"> <img src="/images/other/instagram-png.png" alt=""> </a>
                    <a href="{{actor.twitlink}}"> <img src="/images/other/twitter (1).png" alt=""> </a>
                </div>
                <div class="mv-details" >
                   
                  <span class="readmore-btn">Biograpghy</span>
                      <div class="story-readmore">

                        <span class="closee"> &#10006; </span>
                        
                        <p>{{actor.biography}}</p>
                    </div>
                </div>
                                    
            </div>
            
        </div>
        <div class="black-header">
          <h2>{{actor.actorname}} FILMOGRAPHY</h2>
        </div>
        
      
