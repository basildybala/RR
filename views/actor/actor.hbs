<meta property="og:title" content="{{actor.actorname}}">
<meta property="og:site_name" content="DramaLite.Com Actor Section ">
<meta property="og:url" content="https://dramalite.com/actor/{{actor._id}}">
<meta property="og:description" content="Language:{{actor.language}}, Age:{{actor.age}}, Biography:{{actor.biography}}">
<meta property="og:type" content="video.movie">
<meta property="og:image" content="https://dramalite.com/images/actor-posters/{{actor._id}}.jpg">
<title>{{actor.actorname}} Movies& Age Birthday All Details in മലയാളം-Dramalite</title>
<script type="application/ld+json">
{
  "@context": "https://schema.org/",
  "@type": "Person",
  "name": "{{actor.actorname}}",
  "url": "https://dramalite.com/actor/{{actor._id}}",
  "image": "https://dramalite.com/images/actor-posters/{{actor._id}}.jpg",
  "sameAs": "{{actor.instalink}}",
  "jobTitle": "Actors",
  "worksFor": {
    "@type": "Organization",
    "name": "Film Industry"
  }  
}
</script>

