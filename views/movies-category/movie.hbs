    <!-----------------------------------MOVIES SECTION----------------------->
<div class="main">
  <div class="movies-english-header"><h1>{{movie.engname}}</h1></div>
  <div class="movies-images">
    <img
      data-src="/images/movie-posters/{{movie._id}}a.jpg"
      class="backimg lazy-photo"
      alt="{{movie.name}}.jpg"
    />
    <a href="/images/movie-posters/{{movie._id}}.jpg">
      <img
        data-src="/images/movie-posters/{{movie._id}}.jpg"
        class="movie-cover lazy-photo"
        alt="{{movie.name}}coverimage.jpg"
      /></a>
    <div class="trailor-btn">
      <div class="fa-trailor">
        {{#if movie.yttrailorlink}}
          <p> Watch Trailor</p>
        {{else}}
          <p> Trailor Notyet</p>
        {{/if}}</div>
      <div class="fa fa-playy"> <i class="fa fa-play"></i> </div></div>
    <div class="video-container">
      <span class="close"> &#10006; </span>
      <iframe
        class="lazy-photo"
        data-src="{{movie.yttrailorlink}}"
        frameborder="0"
        allow="autoplay"
        allowfullscreen
      ></iframe>
    </div>
  </div>
  <div class="movies-malayalam-header"><i class="fas fa-star"></i><h6
    >{{movie.name}}</h6></div>

  <div class="movie-details-section">
    <div class="basic-head">
            <h4>Details</h4>
          </div>
          <div class="details">           
            <div class="details-col">
              <h5>Language:</h5>
              <p>{{movie.category}}</p>
            </div>
            <div class="details-col">
              <h5>Genre:</h5>
              <p>{{movie.genre}}</p>
            </div>
            <div class="details-col">
              <h5>Director:</h5>
              <p>{{movie.director}}</p>
            </div>
            <div class="details-col">
              <h5>Produced By:</h5>
              <p>{{movie.producedby}}</p>
            </div>
            <div class="details-col">
              <h5>Release Date:</h5>
              {{#if movie.releasedate}}
                <p>{{movie.releasedate}}</p>{{else}}
                <p>Date Not Confirm</p>
              {{/if}}
            </div>
            <div class="details-col">
              <h5>Length:</h5>
              <p>{{movie.duration}}</p>
            </div>
            <div class="details-col">   
              <div class="share-link">
                <h6>Share:</h6>
                <a
                  href="tg://msg?text= https://dramalite.com/movie/{{movie._id}}   "
                  data-action="share/tg/share"
                  class="share-telegram"
                ><i class="fa fa-telegram" aria-hidden="true"></i></a>
                <a
                  href="whatsapp://send?text= https://dramalite.com/movie/{{movie._id}}"
                  data-action="share/whatsapp/share"
                  class="share-whatsapp"
                ><i class="fa fa-whatsapp" aria-hidden="true"></i></a>
              </div>  
            </div>
          </div>
<!----------------Story------------->
 <div class="movie-story">
            <div class="basic-head">
              <h4>Story</h4>
            </div>
            <div class="story-p">
